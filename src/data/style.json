{
  "version": 8,
  "name": "Coffee Milk Beer PMTiles Style",
  "sources": {
    "base-tiles": {
      "type": "vector",
      "url": "pmtiles:/pmtiles/base-tiles.pmtiles"
    },
    "isochrones": {
      "type": "vector",
      "url": "pmtiles:/pmtiles/isochrones.pmtiles"
    }
  },
  "sprite": "",
  "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#f8fafc"
      }
    },
    {
      "id": "base-tiles",
      "type": "fill",
      "source": "base-tiles",
      "source-layer": "landuse",
      "paint": {
        "fill-color": "#e2e8f0",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "base-tiles",
      "source-layer": "transportation",
      "paint": {
        "line-color": "#ffffff",
        "line-width": 1
      }
    }
  ]
}
